{
    "name": "croudtech/recurring-task-scheduler",
    "description": "Laravel Recurring Task Scheduler",
    "type": "laravel-library",
    "license": "proprietary",
    "authors": [{
        "name": "Croud Tech",
        "email": "tech@croud.co.uk"
    }],
    "autoload": {
        "psr-4": {
            "CroudTech\\RecurringTaskScheduler\\": "src"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests"
        ],
        "psr-4": {
            "CroudTech\\RecurringTaskScheduler\\Tests\\\\": "tests",
            "CroudTech\\RecurringTaskScheduler\\Tests\\App\\": "tests/app"
        }
    },
    "minimum-stability": "dev",
    "require-dev": {
        "phpunit/phpunit": "^5.5"
    },
    "require": {
        "composer/installers": "~1.0",
        "croudtech/laravel-repositories": "^1.5",
        "laravel/laravel": "5.3.*",
        "nesbot/carbon": "^1.21"
    }
}