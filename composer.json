{
    "name": "croudtech/package",
    "description": "Laravel package boiler plate",
    "type": "composer",
    "license": "MIT",
    "authors": [{
        "name": "Croud Tech",
        "email": "tech@croud.co.uk"
    }],
    "autoload": {
        "psr-4": {
            "CroudTech\\RecurringTaskScheduler\\": "src"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests"
        ],
        "psr-4": {
            "CroudTech\\RecurringTaskScheduler\\Tests\\": "tests",
            "CroudTech\\RecurringTaskScheduler\\Tests\\App": "tests/app"
        }
    },
    "minimum-stability": "dev",
    "require-dev": {
        "phpunit/phpunit": "^5.5"
    },
    "require": {
        "laravel/laravel": "5.3.*",
        "nesbot/carbon": "^1.23@dev",
        "croudtech/laravel-repositories": "dev-master"
    }
}